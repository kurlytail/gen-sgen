{
    "name": "@kurlytail/gen-sgen",
    "description": "SGEN project generator",
    "version": "0.0.1",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.1.0",
        "babel-loader": "^8.0.5",
        "babel-plugin-module-resolver": "^3.1.3",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-register": "^6.26.0",
        "child_process": "^1.0.2",
        "eslint": "^5.13.0",
        "eslint-config-prettier": "^4.0.0",
        "eslint-import-resolver-babel-module": "^5.0.1",
        "eslint-loader": "^2.1.2",
        "eslint-plugin-babel": "^5.3.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jest": "^22.2.1",
        "eslint-plugin-prettier": "^3.0.1",
        "jest": "^24.1.0",
        "jest-junit-reporter": "^1.1.0",
        "jsdoc": "^3.5.5",
        "prettier": "^1.16.2"
    },
    "license": "UNLICENSED",
    "jest": {
        "testResultsProcessor": "./node_modules/jest-junit-reporter",
        "moduleDirectories": [
            "node_modules",
            "./src/js"
        ],
        "collectCoverage": true,
        "coverageReporters": [
            "text",
            "html"
        ],
        "collectCoverageFrom": [
            "src/js/**/*.js",
            "!**/node_modules/**"
        ],
        "testPathIgnorePatterns": [
            "testoutput",
            ".generated"
        ]
    },
    "scripts": {
        "docs": "jsdoc -c jsdoc.json && npx opn-cli ./docs/index.html",
        "lint": "eslint src",
        "test": "jest tests",
        "test:watch": "jest --watch tests",
        "test:ci": "jest --ci tests",
        "clean": "rimraf ./docs ./dist ./coverage test-report.xml"
    },
    "dependencies": {
        "@kurlytail/gen-lib": "^0.0.73"
    },
    "bin": {
        "sgen-sgen": "scripts/sgen-sgen.sh"
    }
}
