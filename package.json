{
    "name": "@kurlytail/gen-sgen",
    "description": "SGEN project generator",
    "version": "1.3.168",
    "main": "dist/sgen.min.js",
    "devDependencies": {
        "@babel/cli": "7.12.10",
        "@babel/core": "7.12.10",
        "@babel/plugin-proposal-class-properties": "7.12.1",
        "@babel/plugin-proposal-object-rest-spread": "7.12.1",
        "@babel/preset-env": "7.12.11",
        "@babel/preset-flow": "7.12.1",
        "@babel/register": "7.12.10",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.1.0",
        "babel-jest": "26.6.3",
        "babel-loader": "8.2.2",
        "babel-plugin-module-resolver": "4.1.0",
        "child_process": "1.0.2",
        "circular-dependency-plugin": "5.2.2",
        "eslint": "7.16.0",
        "eslint-config-prettier": "7.1.0",
        "eslint-import-resolver-babel-module": "5.2.0",
        "eslint-loader": "4.0.2",
        "eslint-plugin-babel": "5.3.1",
        "eslint-plugin-jest": "24.1.3",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-prettier": "3.3.0",
        "jest": "26.6.3",
        "jest-junit-reporter": "1.1.0",
        "jsdoc": "3.6.6",
        "prettier": "2.2.1",
        "source-map-loader": "2.0.0",
        "uglifyjs-webpack-plugin": "2.2.0",
        "webpack": "5.11.0",
        "webpack-cli": "3.3.12",
        "webpack-node-externals": "2.5.2"
    },
    "license": "UNLICENSED",
    "jest": {
        "testResultsProcessor": "./node_modules/jest-junit-reporter",
        "moduleDirectories": [
            "node_modules",
            "./src/js"
        ],
        "collectCoverage": true,
        "coverageReporters": [
            "text",
            "html"
        ],
        "collectCoverageFrom": [
            "src/js/**/*.js",
            "!**/node_modules/**"
        ],
        "testPathIgnorePatterns": [
            "testoutput",
            ".generated"
        ]
    },
    "scripts": {
        "docs": "jsdoc -c jsdoc.json && npx opn-cli ./docs/index.html",
        "build": "NODE_ENV=\"debug\" webpack --display-modules --display-reasons",
        "lint": "eslint src",
        "test": "jest tests",
        "test:watch": "jest --watch tests",
        "test:ci": "jest --ci tests",
        "clean": "rimraf ./docs ./dist ./coverage test-report.xml"
    },
    "dependencies": {
        "@kurlytail/gen-lib": "^2.1.8",
        "yaml": "1.10.0"
    }
}
