{
    "name": "@kurlytail/gen-sgen",
    "description": "SGEN project generator",
    "version": "1.3.54",
    "main": "dist/sgen.min.js",
    "devDependencies": {
        "@babel/cli": "7.6.4",
        "@babel/core": "7.6.4",
        "@babel/plugin-proposal-class-properties": "7.5.5",
        "@babel/plugin-proposal-object-rest-spread": "7.6.2",
        "@babel/preset-env": "7.6.3",
        "@babel/preset-flow": "7.0.0",
        "@babel/register": "7.6.2",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "10.0.3",
        "babel-jest": "24.9.0",
        "babel-loader": "8.0.6",
        "babel-plugin-module-resolver": "3.2.0",
        "child_process": "1.0.2",
        "circular-dependency-plugin": "5.2.0",
        "eslint": "6.6.0",
        "eslint-config-prettier": "6.4.0",
        "eslint-import-resolver-babel-module": "5.1.0",
        "eslint-loader": "3.0.2",
        "eslint-plugin-babel": "5.3.0",
        "eslint-plugin-jest": "22.20.0",
        "eslint-plugin-import": "2.18.2",
        "eslint-plugin-prettier": "3.1.1",
        "jest": "24.9.0",
        "jest-junit-reporter": "1.1.0",
        "jsdoc": "3.6.3",
        "prettier": "1.18.2",
        "source-map-loader": "0.2.4",
        "uglifyjs-webpack-plugin": "2.2.0",
        "webpack": "4.41.2",
        "webpack-cli": "3.3.9",
        "webpack-node-externals": "1.7.2"
    },
    "license": "UNLICENSED",
    "jest": {
        "testResultsProcessor": "./node_modules/jest-junit-reporter",
        "moduleDirectories": [
            "node_modules",
            "./src/js"
        ],
        "collectCoverage": true,
        "coverageReporters": [
            "text",
            "html"
        ],
        "collectCoverageFrom": [
            "src/js/**/*.js",
            "!**/node_modules/**"
        ],
        "testPathIgnorePatterns": [
            "testoutput",
            ".generated"
        ]
    },
    "scripts": {
        "docs": "jsdoc -c jsdoc.json && npx opn-cli ./docs/index.html",
        "build": "NODE_ENV=\"debug\" webpack --display-modules --display-reasons",
        "lint": "eslint src",
        "test": "jest tests",
        "test:watch": "jest --watch tests",
        "test:ci": "jest --ci tests",
        "clean": "rimraf ./docs ./dist ./coverage test-report.xml"
    }
}
