<%
    var genName = design.MasterDesign[0].name.replace('gen-', '');
%>
{
    "name": "@kurlytail/gen-<%=genName%>",
    "description": "<%=genName.toUpperCase()%> project generator",
    "version": "0.0.1",
    "main": "dist/<%=genName%>.min.js",
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.0.1",
        "babel-jest": "^23.6.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-module-resolver": "^3.1.1",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-flow": "^6.23.0",
        "babel-register": "^6.26.0",
        "child_process": "^1.0.2",
        "circular-dependency-plugin": "^4.2.1",
        "eslint": "^4.19.1",
        "eslint-config-prettier": "^2.6.0",
        "eslint-import-resolver-babel-module": "^4.0.0",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-babel": "^4.1.2",
        "eslint-plugin-flowtype": "^3.4.2",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jest": "^22.1.2",
        "eslint-plugin-prettier": "^2.3.1",
        "flow-bin": "^0.94.0",
        "jest": "^23.6.0",
        "jest-junit-reporter": "^1.1.0",
        "jsdoc": "^3.5.5",
        "prettier": "^1.16.0",
        "source-map-loader": "^0.2.3",
        "webpack": "^3.5.5",
        "webpack-node-externals": "^1.7.2"
    },
    "license": "UNLICENSED",
    "jest": {
        "testResultsProcessor": "./node_modules/jest-junit-reporter",
        "moduleDirectories": [
            "node_modules",
            "./src/js"
        ],
        "collectCoverage": true,
        "coverageReporters": [
            "text",
            "html"
        ],
        "collectCoverageFrom": [
            "src/js/**/*.js",
            "!**/node_modules/**"
        ],
        "testPathIgnorePatterns": [
            "testoutput",
            ".generated"
        ]
    },
    "scripts": {
        "docs": "jsdoc -c jsdoc.json && npx opn-cli ./docs/index.html",
        "build": "NODE_ENV=\"debug\" webpack --display-modules --display-reasons",
        "lint": "eslint src",
        "test": "jest tests",
        "test:watch": "jest --watch tests",
        "test:ci": "jest --ci tests",
        "clean": "rimraf ./docs ./dist ./coverage test-report.xml"
    },
    "dependencies": {
        "@kurlytail/gen-lib": "^1.0.7"
    }
}